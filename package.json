{
  "name": "@connexta/atlas",
  "description": "Atlas",
  "author": "Connexta LLC",
  "version": "0.0.35",
  "private": false,
  "scripts": {
    "copy:package": "cpx package.json dist",
    "prebuild": "rimraf dist && yarn copy:package && yarn storybook:build && cpx target/storybook dist",
    "build": "tsc",
    "publish:npm": "yarn build && npm publish dist",
    "storybook": "ace storybook --root atoms",
    "storybook:build": "ace storybook --root atoms --static",
    "start": "yarn storybook",
    "watch:ace": "cpx -w -v \"../ace/{,!(node_modules)/**/}*{.js,.tsx}\" ./node_modules/@connexta/ace/"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:connexta/atlas.git"
  },
  "license": "LGPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@connexta/ace": "git://github.com/connexta/ace.git#14f9fa9e80903ea205cff8d1e4697d445e7d5779",
    "@types/backbone": "1.3.45",
    "@types/backbone.marionette": "3.3.7",
    "@types/d3": "5.7.1",
    "@types/lodash.throttle": "4.1.5",
    "@types/moment": "2.13.0",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.0.3",
    "backbone": "1.3.3",
    "backbone.marionette": "2.4.7",
    "cpx": "1.5.0",
    "d3": "5.9.1",
    "density-clustering": "1.3.0",
    "husky": "1.3.1",
    "lodash.throttle": "4.1.1",
    "moment": "2.22.0",
    "moment-timezone": "0.5.16",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rimraf": "2.6.3",
    "storybook-addon-styled-component-theme": "1.1.1",
    "tslint": "5.12.1",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "@material-ui/core": "4.3.1",
    "@material-ui/icons": "4.2.1",
    "html-react-parser": "0.6.1",
    "styled-components": "4.3.2",
    "ts-loader": "6.0.4"
  },
  "peerDependencies": {
    "backbone": ">= 1 < 2",
    "backbone.marionette": ">= 1 < 3",
    "d3": ">= 5 < 6",
    "moment": ">= 2 < 3",
    "moment-timezone": "<=1",
    "react": ">= 16.8.6 < 17",
    "react-dom": ">= 16.8.6 < 17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "typings": "./@types/density-clustering/index.d.ts"
}
